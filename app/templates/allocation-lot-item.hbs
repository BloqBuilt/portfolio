<div class="row-container lot">
  <div class="cell holding-name">{{moment model.acquiredAt 'YYYY-MM-DD'}}</div>
  <div class="cell current-weight">{{format-percentage currentWeight}}</div>
  <div class="cell weight-graph shares">{{format-decimal model.quantityRemaining 4}} shares</div>
  <div class="cell value-change">
    <span class="{{if model.isLoss 'red' 'green'}}">
      {{format-currency model.unrealizedValueChange}}
    </span>
  </div>
  <div class="cell current-value">{{format-currency marketValue}}</div>
</div>
